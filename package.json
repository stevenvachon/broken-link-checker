{
  "name": "broken-link-checker",
  "description": "Find broken links, missing images, etc within your HTML.",
  "version": "0.8.0",
  "license": "MIT",
  "author": "Steven Vachon <contact@svachon.com> (https://svachon.com)",
  "repository": "github:stevenvachon/broken-link-checker",
  "main": "lib-cjs",
  "bin": {
    "blc": "bin/blc",
    "broken-link-checker": "bin/blc"
  },
  "dependencies": {
    "auto-tunnel": "github:stevenvachon/auto-tunnel",
    "calmcard": "~0.1.1",
    "chalk": "^2.4.2",
    "condense-whitespace": "^2.0.0",
    "deep-freeze-node": "^1.1.3",
    "default-user-agent": "^1.0.0",
    "errno": "~0.1.7",
    "gauge": "^2.7.4",
    "got": "^9.6.0",
    "http-equiv-refresh": "^2.0.1",
    "humanize-duration": "^3.20.0",
    "is-object": "^1.0.1",
    "is-promise": "^2.1.0",
    "is-stream": "^2.0.0",
    "is-string": "^1.0.4",
    "isurl": "^4.0.1",
    "keyscan": "^1.7.0",
    "limited-request-queue": "^4.0.0",
    "link-types": "^3.0.0",
    "list-to-array": "^1.1.0",
    "lodash": "^4.17.14",
    "longest": "^2.0.1",
    "node-notifier": "^5.4.0",
    "optionator": "~0.8.2",
    "parse-srcset": "^1.0.2",
    "parse5": "^4.0.0",
    "robot-directives": "~0.4.0",
    "robots-txt-guard": "~0.2.1",
    "robots-txt-parse": "^1.0.1",
    "strip-ansi": "^5.2.0",
    "supports-semigraphics": "^1.0.1",
    "url-relation": "github:stevenvachon/url-relation",
    "urlcache": "github:stevenvachon/urlcache",
    "walk-parse5": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-optional-catch-binding": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-private-methods": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "babel-plugin-add-module-exports": "^1.0.2",
    "basic-auth": "^2.0.1",
    "basic-auth-header": "^1.0.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "github:stevenvachon/chai-subset",
    "chai-things": "~0.2.0",
    "coveralls": "^3.0.5",
    "escape-string-regexp": "^2.0.0",
    "eslint": "^6.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.2",
    "mocha": "^6.1.4",
    "nock": "^11.0.0-beta.25",
    "nyc": "^14.1.1",
    "void-elements": "^3.1.0"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "build": "babel lib/ --config-file=./scripts/babel.config.js --out-dir=lib-cjs/ --source-maps",
    "ci": "npm test && nyc report --reporter=text-lcov | coveralls",
    "lint": "npm run lint:cjs && npm run lint:esm",
    "lint:cjs": "eslint --config=scripts/eslintrc.cjs.js scripts/",
    "lint:esm": "eslint --config=scripts/eslintrc.esm.js --ignore-pattern=lib-es5/** --ignore-pattern=scripts/** .",
    "posttest": "nyc report --reporter=text-summary --reporter=html",
    "prepublishOnly": "npm test && npm run build",
    "test": "nyc --exclude=scripts/ --silent mocha test/ --bail --check-leaks --require=scripts/register-babel",
    "test:watch": "npm test -- --watch --watch-extensions=json"
  },
  "files": [
    "bin",
    "lib",
    "lib-cjs"
  ],
  "keywords": [
    "404",
    "html",
    "hyperlink",
    "links",
    "seo",
    "url",
    "whatwg"
  ]
}
